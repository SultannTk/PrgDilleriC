         char sayfasayisi[5]="354";
         
         
         
         printf("kitap adi: %s \n",kitapadi );
         printf("yazari: %s \n",yazar);
         printf("basim yili: %s \n",basimyili);
         printf("sayfa sayisi: %s",sayfasayisi );







